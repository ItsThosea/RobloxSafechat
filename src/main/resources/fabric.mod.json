{
  "schemaVersion": 1,
  "id": "robloxsafechat",
  "version": "${version}",

  "name": "RobloxSafechat",
  "description": "Roblox's old safechat feature from 2007-2014 now in Minecraft!",
  "authors": ["Tiny Games - idea, icons & all default messages", "Thosea - programming"],
  "license": "MIT",
  "icon": "assets/robloxsafechat/icon.png",
  "entrypoints": {
    "client": [
      "me.thosea.robloxsafechat.RobloxSafechat"
    ]
  },
  "environment": "client",
  "depends": {
    "minecraft": ">=1.20.2"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.source": "https://github.com/TinyGamesCoding/MC-Safechat",
        "modmenu.issues": "https://github.com/TinyGamesCoding/MC-Safechat/issues",
        "modmenu.wiki": "https://github.com/TinyGamesCoding/MC-Safechat/wiki",
        "modmenu.modrinth": "https://modrinth.com/mod/robloxsafechat/"
      },
      "update_checker": true
    }
  },
  "mixins": [
    "robloxsafechat.mixins.json"
  ]
}
